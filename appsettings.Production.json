{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Error"
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  },
  "Auth0": {
    "Domain": "${AUTH0_DOMAIN}",
    "ClientId": "${AUTH0_CLIENT_ID}",
    "ClientSecret": "${AUTH0_CLIENT_SECRET}",
    "Audience": "${AUTH0_AUDIENCE}"
  }
}
