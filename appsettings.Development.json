{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HttpsRedirection": {
    "HttpsPort": 7299
  },
  "Auth0": {
    "Domain": "${AUTH0_DOMAIN}",
    "ClientId": "${AUTH0_CLIENT_ID}",
    "ClientSecret": "${AUTH0_CLIENT_SECRET}",
    "Audience": "${AUTH0_AUDIENCE}",
    "RedirectUri": "${AUTH0_REDIRECT_URI}"
  }
}
